{
  "name": "rex",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "devel": "npm run testsy & npm run server",
    "test": "tape -r babel-register -r css-modules-require-hook spec/**/*.js | tap-spec",
    "test-min": "tape -r babel-register -r css-modules-require-hook spec/**/*.js | tap-dot",
    "tests": "npm run test --silent & nodemon -w src -w spec -e js,html,css --no-colors -q -C -x 'npm run test-min --silent'",
    "server": "nodemon -w src -w spec/api.yaml -e js,html,css --no-colors -q index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerdlabs/rex.git"
  },
  "bugs": {
    "url": "https://github.com/nerdlabs/rex/issues"
  },
  "homepage": "https://github.com/nerdlabs/rex#readme",
  "devDependencies": {
    "babel-eslint": "5.0.0-beta6",
    "csslint": "0.10.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "jspm": "0.16.25",
    "nodemon": "1.8.1",
    "react-addons-test-utils": "0.14.6",
    "tap-dot": "1.0.1",
    "tap-spec": "4.1.1",
    "tape": "4.4.0"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-strict-mode": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.4.3",
    "compression": "1.6.1",
    "css-modules-require-hook": "2.1.0",
    "css-modulesify": "0.16.1",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "git-rev": "0.2.1",
    "isomorphic-fetch": "2.2.1",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "react-router": "2.0.0-rc5",
    "redux": "3.0.6",
    "redux-actions": "0.9.1",
    "redux-promise": "0.5.1",
    "redux-simple-router": "2.0.4",
    "swagger-express-middleware": "1.0.0-alpha.12",
    "throng": "1.0.1"
  },
  "jspm": {
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.20",
      "isomorphic-fetch": "npm:isomorphic-fetch@^2.2.1",
