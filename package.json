{
  "name": "rex",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run start-prod; else npm run start-dev; fi",
    "start-prod": "npm run build; node index.js",
    "start-dev": "npm run test; npm run tests & npm run watch & npm run server",
    "test": "tape -r babel-register -r css-modules-require-hook spec/**/*.js | tap-dot",
    "tests": "nodemon -w src -w spec -e js,html,css --no-colors -q -C -x 'npm run test --silent'",
    "server": "nodemon -w src -w spec/api.yaml -e js,html,css --no-colors -q index.js",
    "watch": "webpack -w",
    "build": "webpack -p",
    "postinstall": "mkdir -p dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerdlabs/rex.git"
  },
  "bugs": {
    "url": "https://github.com/nerdlabs/rex/issues"
  },
  "homepage": "https://github.com/nerdlabs/rex#readme",
  "devDependencies": {
    "babel-eslint": "5.0.0-beta6",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "exorcist": "0.4.0",
    "nodemon": "1.8.1",
    "react-addons-test-utils": "0.14.7",
    "tap-dot": "1.0.1",
    "tape": "4.4.0"
  },
  "dependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-strict-mode": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.4.3",
    "compression": "1.6.1",
    "css-loader": "0.23.1",
    "css-modules-require-hook": "2.1.0",
    "csswring": "4.2.1",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "isomorphic-fetch": "2.2.1",
    "normalize.css": "3.0.3",
    "postcss": "5.0.14",
    "postcss-cssnext": "2.4.0",
    "postcss-loader": "0.8.0",
    "postcss-modules-extract-imports": "1.0.0",
    "postcss-modules-local-by-default": "1.0.1",
    "postcss-modules-scope": "1.0.0",
    "postcss-modules-values": "1.1.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.1.2",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "2.1.0",
    "redux": "3.1.4",
    "redux-actions": "0.9.1",
    "redux-promise": "0.5.1",
    "style-loader": "0.13.0",
    "swagger-express-middleware": "1.0.0-alpha.12",
    "throng": "1.0.1",
    "webpack": "1.12.12"
  }
}
