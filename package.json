{
  "name": "rex",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "test": "tape -r babel-register -r css-modules-require-hook spec/**/*.js",
    "start": "NODE_ENV=production node index.js",
    "devel": "npm run tests & npm run watch & npm run server",
    "tests": "nodemon -w src -w spec -e js,html,css --no-colors -q -x 'npm test'",
    "server": "nodemon -w src -w spec/api.yaml -e js,html,css --no-colors -q index.js",
    "watch": "watchify -d -t babelify -p [css-modulesify --after postcss-cssnext -o dist/main.css] -o dist/main.js src/client.js",
    "prestart": "NODE_ENV=production browserify -t babelify -g envify -g [uglifyify -x .js] -p [css-modulesify --after postcss-cssnext --after csswring -o dist/main.css] -o dist/main.js src/client.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmbch/rex.git"
  },
  "bugs": {
    "url": "https://github.com/dmbch/rex/issues"
  },
  "homepage": "https://github.com/dmbch/rex#readme",
  "devDependencies": {
    "babel-eslint": "4.1.6",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "css-modulesify": "0.16.1",
    "csslint": "0.10.0",
    "csswring": "4.2.1",
    "envify": "3.4.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.15.0",
    "nodemon": "1.8.1",
    "postcss-cssnext": "2.4.0",
    "react-addons-test-utils": "0.14.6",
    "tape": "4.4.0",
    "uglifyify": "3.0.1",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "babel-core": "6.4.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.4.3",
    "compression": "1.6.1",
    "css-modules-require-hook": "2.1.0",
    "ejs": "2.3.4",
    "express": "4.13.3",
    "history": "1.17.0",
    "postcss": "5.0.14",
    "postcss-modules-extract-imports": "1.0.0",
    "postcss-modules-local-by-default": "1.0.1",
    "postcss-modules-scope": "1.0.0",
    "postcss-modules-values": "1.1.1",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "react-router": "1.0.3",
    "redux": "3.0.5",
    "redux-actions": "0.9.0",
    "redux-promise": "0.5.0",
    "redux-simple-router": "2.0.2",
    "swagger-express-middleware": "0.4.7",
    "throng": "1.0.1"
  }
}
